<project name="PyCommon" default="build.egg.PyCommon">

	<property name="application.name" value="PyCommon"/>
	<property name="application.root" value="../PyCommon"/>	
    <property name="application.description" value="Common functionality"/>	
	<import file="../PyUtils/build_pyutils.xml"/>
	
	<target name="build.egg.PyCommon" description="Build PyCommon egg">

		<property name="egg.1" location="../PyCore/dist/PyCore-0.1-py2.7.egg"/>
		<antcall target="check.egg">
			<param name="egg" value="${egg.1}"/>
			<param name="egg.project" value="../PyCore"/>
			<param name="ant.file" value="build_pycore"/>
		</antcall>
		
		<antcall target="build.egg"/>		
	</target>
	
</project>
